<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.quizzapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Quiz1" />
        <activity android:name=".Quiz2" />
        <activity android:name=".Quiz3" />
        <activity android:name=".Quiz4" />
        <activity android:name=".Quiz5" />
        <activity android:name=".QuizScore"></activity>
    </application>

</manifest>